    url('../fonts/sell-icon.ttf?ze40eg') format('truetype'),
    url('../fonts/sell-icon.woff?ze40eg') format('woff'),
    url('../fonts/sell-icon.svg?ze40eg#sell-icon') format('svg')
  font-weight: normal
